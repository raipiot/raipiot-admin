{
  "name": "raipiot-admin",
  "description": "A opinionated React + TypeScript + Vite starter template.",
  "author": "Bruce Song <recall4056@gmail.com> (https://github.com/recallwei/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/raipiot/raipiot-admin"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:staging": "pnpm type:check && NODE_ENV=staging vite build --mode staging",
    "build:prod": "pnpm type:check && NODE_ENV=production vite build --mode production",
    "preview": "vite preview",
    "clean": "rimraf node_modules dist",
    "desktop:dev": "tauri dev",
    "desktop:build:staging": "NODE_ENV=staging tauri build --mode staging",
    "desktop:build:prod": "NODE_ENV=production tauri build --mode production",
    "cspell:check": "cspell --no-progress --show-suggestions --show-context --cache **",
    "type:check": "tsc --pretty --noEmit --composite false",
    "eslint:check": "eslint . --color",
    "eslint:fix": "eslint . --color --fix",
    "prettier:check": "prettier --check --ignore-unknown .",
    "prettier:fix": "prettier --write --ignore-unknown .",
    "cz": "git-cz",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.18.4",
    "@ant-design/happy-work-theme": "^1.0.0",
    "@raipiot-infra/antd": "0.0.5",
    "@raipiot-infra/config": "0.0.2",
    "@raipiot-infra/enums": "^0.0.3",
    "@raipiot-infra/utils": "0.0.3",
    "@tanstack/react-query": "^5.18.0",
    "@tanstack/react-query-devtools": "^5.18.0",
    "@tanstack/react-router": "^1.15.7",
    "@tanstack/router-devtools": "^1.15.7",
    "@tauri-apps/api": "^1.5.3",
    "ahooks": "^3.7.9",
    "antd": "^5.13.3",
    "axios": "^1.6.7",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.10",
    "i18next": "^23.8.1",
    "immer": "^10.0.3",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.1",
    "use-immer": "^0.9.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@iconify/json": "^2.2.177",
    "@iconify/react": "^4.1.1",
    "@raipiot-infra/auto-import": "0.0.8",
    "@raipiot-infra/bootstrap-animation": "0.0.3",
    "@raipiot-infra/commitlint-config": "0.0.2",
    "@raipiot-infra/cspell": "0.0.4",
    "@raipiot-infra/eslint-config": "0.0.3",
    "@raipiot-infra/prettier": "0.0.2",
    "@raipiot-infra/tailwind": "0.0.2",
    "@raipiot-infra/tsconfig": "0.0.2",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@tanstack/router-vite-plugin": "^1.15.4",
    "@tauri-apps/cli": "^1.5.9",
    "@types/lodash-es": "^4.17.12",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.17",
    "commitizen": "^4.3.0",
    "cspell": "^8.3.2",
    "cz-git": "^1.8.0",
    "eslint": "^8.56.0",
    "husky": "^9.0.7",
    "lint-staged": "^15.2.1",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "sass": "^1.70.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-icons": "^0.18.3",
    "unplugin-info": "^1.0.2",
    "vite": "^5.0.12",
    "vite-plugin-compression": "^0.5.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "private": true,
  "license": "MIT"
}
